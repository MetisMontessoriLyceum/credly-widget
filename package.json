{
  "name": "credly-widget",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "mocha",
    "lint": "standard ./bin/www ./app.js ./lib/**/*.js ./routes/**/*.js",
    "setup": "npm install && bower install --allow-root",
    "dev": "gulp",
    "clear-cache": "redis-cli KEYS \"credly-widget:*\" | xargs redis-cli DEL",
    "build": "webpack -p"
  },
  "dependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.13.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "cookie-parser": "^1.3.5",
    "debug": "~2.2.0",
    "express": "^4.13.4",
    "git-rev": "^0.2.1",
    "jade": "~1.11.0",
    "lodash": "^4.15.0",
    "memory-cache": "^0.1.5",
    "morgan": "^1.6.1",
    "proxyquire": "^1.7.10",
    "radium": "^0.19.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-icons": "^2.2.5",
    "redis": "^2.7.1",
    "request": "^2.74.0",
    "then-yield": "0.0.1",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.11.0"
  },
  "devDependencies": {
    "bower": "^1.8.0",
    "gulp": "^3.9.1",
    "gulp-sass": "^2.3.2",
    "mocha": "^3.0.2",
    "standard": "^8.0.0-beta.5"
  }
}
